{
  "name": "dwenzel/reporter-bundle",
  "description": "Test project bundle for the reporter TYPO3 extension",
  "type": "project",
  "homepage": "https://github.com/dwenzel/reporter-bundle",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Dirk Wenzel",
      "email": "der-wenzel@gmx.de"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dwenzel/reporter-api.git"
    }
  ],
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "app/web"
    },
    "helhum/typo3-console": {
      "install-extension-dummy": false
    }
  },
  "config": {
    "vendor-dir": "app/vendor",
    "web-dir": "app/web",
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "cpsit/auditor": "source",
      "dwenzel/reporter": "source",
      "dwenzel/reporter-api": "source",
      "*": "dist"
    }
  },
  "require": {
    "typo3/minimal": "^9.5",
    "typo3/cms-about": "^9.5",
    "typo3/cms-adminpanel": "^9.5",
    "typo3/cms-belog": "^9.5",
    "typo3/cms-beuser": "^9.5",
    "typo3/cms-fluid-styled-content": "^9.5",
    "typo3/cms-form": "^9.5",
    "typo3/cms-impexp": "^9.5",
    "typo3/cms-info": "^9.5",
    "typo3/cms-lowlevel": "^9.5",
    "typo3/cms-reports": "^9.5",
    "typo3/cms-rte-ckeditor": "^9.5",
    "typo3/cms-setup": "^9.5",
    "typo3/cms-sys-note": "^9.5",
    "typo3/cms-t3editor": "^9.5",
    "typo3/cms-tstemplate": "^9.5",
    "typo3/cms-viewpage": "^9.5",
    "helhum/config-loader": "^0.11",
    "helhum/typo3-console": "^5.6",
    "ext-json": "*",
    "cpsit/auditor": "^0.4.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "cpsit/auditor": "dev-develop as 0.4.0",
    "dwenzel/reporter": "dev-develop as 0.4.0",
    "dwenzel/reporter-api": "dev-develop",
    "nimut/testing-framework": "^4.1"
  },
  "autoload-dev": {
    "psr-4": {
      "DWenzel\\Reporter\\Tests\\": "app/web/typo3conf/ext/reporter/Tests",
      "DWenzel\\ReporterApi\\": "app/vendor/dwenzel/reporter-api/src",
      "DWenzel\\ReporterApi\\Tests\\": "app/vendor/dwenzel/reporter-api/tests"
    }
  },
  "scripts": {
    "typo3-cms-scripts": [
      "typo3cms install:fixfolderstructure",
      "typo3cms install:generatepackagestates"
    ],
    "post-autoload-dump": [
      "@typo3-cms-scripts"
    ]
  }
}
